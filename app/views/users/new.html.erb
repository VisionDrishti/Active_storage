<h1>Users#new</h1>
<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :name, "Name" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :avatar, "Avatar" %>
    <%= form.file_field :avatar %>
  </div>
  <div class="photos">
  <%= form.label :pics %>
  <%= form.file_field :pics, :multiple => true %>
</div>
  <div>
    <%= form.submit "Submit" %>
  </div>
<% end %>